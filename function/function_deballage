def function(*args, **kwargs):
    print(args)
    print(kwargs)

tuple = (1, 2, 3, 4, 5, 6, 7, 8)
dictionnaire = {"a":"a", "b":"b", "c":"c", "d":"d","e":"e"}

function(1,2,3,4,5,6,7,8,  a="a", b="b", c="c", d="d", e="e")
function(*tuple, **dictionnaire)

(1, 2, 3, 4, 5, 6, 7, 8)
{'a': 'a', 'b': 'b', 'c': 'c', 'd': 'd', 'e': 'e'}
(1, 2, 3, 4, 5, 6, 7, 8)
{'a': 'a', 'b': 'b', 'c': 'c', 'd': 'd', 'e': 'e'}

#function(tuple, dictionnaire)